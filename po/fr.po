#: ../ailurus/common/tips.py:28
msgid ""
"motto of Ailurus\n"
"Give a man a fish and he can eat for a day; but if you teach him how to "
"fish, he'll eat for a lifetime."
msgstr ""
"la devise d'Ailurus\n"
"Donne un poisson à un homme, et il mangera un jour; mais apprends lui à "
"pêcher, et il mangera toute sa vie."

#: ../ailurus/common/tips.py:31
msgid ""
"Clear Bash history\n"
"history -c"
msgstr ""
"Vider l'historique du Bash\n"
"history -c"

#: ../ailurus/common/tips.py:34
msgid ""
"If you want to use colorful \"ls\", that is, use colors to distinguish types "
"of files, you can add these lines in ~/.bashrc:\n"
"\n"
"if [ \"$TERM\" != \"dumb\" ]; then\n"
"    eval \"`dircolors -b`\"\n"
"    alias ls='ls --color=auto'\n"
"fi"
msgstr ""
"Si vous voulez utiliser un \"ls\" coloré, qui utilise des couleurs "
"distinctes pour les types de fichier. Vous pouvez ajouter ces lignes dans "
"~/.bashrc:\n"
"\n"
"if [ \"$TERM\" != \"dumb\" ]; then\n"
"eval \"`dircolors -b`\"\n"
"alias ls='ls --color=auto'\n"
"fi"

#: ../ailurus/common/tips.py:41
msgid ""
"Display Linux distributor's ID\n"
"lsb_release -is\n"
"\n"
"Display Linux release number\n"
"lsb_release -rs\n"
"\n"
"Display Linux code name\n"
"lsb_release -cs"
msgstr ""
"Afficher le nom de la distribution Linux\n"
"lsb_release -is\n"
"\n"
"Afficher la version de la distribution Linux\n"
"lsb_release -rs\n"
"\n"
"Afficher le nom de code de la distribution Linux\n"
"lsb_release -cs"

#: ../ailurus/common/tips.py:51
msgid ""
"Display machine hardware name\n"
"uname -m"
msgstr ""
"Afficher le type de configuration matérielle\n"
"uname -m"

#: ../ailurus/common/tips.py:54
msgid ""
"List all PCI devices, such as display card and ethernet card.\n"
"lspci\n"
msgstr ""
"Liste tous les composants PCI, comme une carte graphique ou une carte "
"réseau.\n"
"lspci\n"

#: ../ailurus/common/tips.py:58
msgid ""
"Reclaim memory which stores pagecache, dentries and inodes\n"
"echo 3 > /proc/sys/vm/drop_caches\n"
msgstr ""
"Nécessite que la mémoire stocke pagecache, dentries et inodes\n"
"echo 3 > /proc/sys/vm/drop_caches\n"

#: ../ailurus/common/tips.py:62
msgid ""
"Display a list of modules in the Linux Kernel\n"
"lsmod\n"
msgstr ""
"Affiche une liste des modules du noyaux Linux\n"
"lsmod\n"

#: ../ailurus/common/tips.py:66
msgid ""
"List USB devices\n"
"lsusb -v\n"
msgstr ""
"Liste les périphériques USB\n"
"lsusb -v\n"

#: ../ailurus/common/tips.py:70
msgid ""
"Display the status of ethernet card\n"
"sudo ethtool eth0\n"
msgstr ""
"Affiche le statut d'une carte ethernet\n"
"sudo ethtool eth0\n"

#: ../ailurus/common/tips.py:74
msgid ""
"List hardware\n"
"sudo lshw\n"
msgstr ""
"Liste le matériel\n"
"sudo lshw\n"

#: ../ailurus/common/tips.py:78
msgid ""
"List harddisk partitions\n"
"sudo fdisk -l\n"
msgstr ""
"Liste les partitions du disque dur\n"
"sudo fdisk -l\n"

#: ../ailurus/common/tips.py:82
msgid ""
"Display SATA harddisk parameters\n"
"sudo hdparm -I /dev/sda\n"
msgstr ""
"Affiche les paramètres d'une disque dur SATA\n"
"sudo hdparm -l /dev/sda\n"

#: ../ailurus/common/tips.py:86
msgid ""
"Display disk space usage\n"
"df -h\n"
msgstr ""
"Affiche l'utilisation de l'espace du disque\n"
"df -h\n"

#: ../ailurus/common/tips.py:90
msgid ""
"Display file/folder space usage\n"
"du -bsh FOLDER_NAME\n"
msgstr ""
"Affiche l'utilisation de l'espace du fichier/dossier\n"
"du -bsh NOM_DU_DOSSIER\n"

#: ../ailurus/common/tips.py:94
msgid ""
"Display amount of free and used memory\n"
"free\n"
msgstr ""
"Affiche la quantité de mémoire libre et occupée\n"
"free\n"

#: ../ailurus/common/tips.py:98
msgid ""
"Display processes\n"
"ps -e\n"
"Display a tree of processes\n"
"pstree\n"
"Display processes dynamically\n"
"top\n"
msgstr ""
"Affiche les processus\n"
"ps-e\n"
"Affiche un arbre des processus\n"
"pstree\n"
"Affiche les processus dynamiquement\n"
"top\n"

#: ../ailurus/common/tips.py:106
msgid ""
"Terminate a process with a given process id\n"
"sudo kill -9 PROCESS_ID\n"
msgstr ""
"Termine un processus avec un l'identificateur du processus donné\n"
"sudo kill -9 IDENTIFICATEUR_DU_PROCESSUS\n"

#: ../ailurus/common/tips.py:110
msgid ""
"Terminate all processes with a given name\n"
"sudo killall PROCESS_NAME\n"
msgstr ""
"Termine tout les processus avec un nom donné\n"
"sudo killall NOM_DU_PROCESSUS\n"

#: ../ailurus/common/tips.py:114
msgid ""
"List files which are opened by a given process\n"
"lsof -p PROCESS_ID\n"
"lsof -c PROCESS_NAME\n"
msgstr ""
"Liste les fichiers ouverts par un processus donné\n"
"lsof -p IDENTIFICATEUR_DU_PROCESSUS\n"
"lsof -c NOM_DU_PROCESSUS\n"

#: ../ailurus/common/tips.py:119
msgid ""
"List processes which opened a given file\n"
"lsof FILE_NAME\n"
msgstr ""
"Liste les processus avec le fichier ouvert donné\n"
"lsof NOM_FICHIER\n"

#: ../ailurus/common/tips.py:123
msgid ""
"List processes which are using port 80\n"
"lsof -i :80\n"
msgstr ""
"Liste les processus utilisant le port 80\n"
"lsof -i :80\n"

#: ../ailurus/common/tips.py:127
msgid ""
"Configure an ADSL connection\n"
"sudo pppoeconf\n"
msgstr ""
"Configurer une connexion ADSL\n"
"sudo pppoeconf\n"

#: ../ailurus/common/tips.py:131
msgid ""
"Starts up ADSL connections\n"
"sudo pon\n"
"Shuts down ADSL connections\n"
"sudo poff\n"
msgstr ""
"Démarre les connections ADSL\n"
"sudo pon\n"
"Arrête les connections ADSL\n"
"sudo poff\n"

#: ../ailurus/common/tips.py:137
msgid ""
"Display MAC of a given IP address\n"
"arping IP_ADDRESS\n"
msgstr ""
"Affiche l'adresse MAC d'une adresse IP donnée\n"
"arping ADRESSE_IP\n"

#: ../ailurus/common/tips.py:141
msgid ""
"Display NetBIOS name of a given IP address\n"
"nmblookup -A IP_ADDRESS\n"
msgstr ""
"Affiche le nom NetBIOS d'une adresse IP donnée\n"
"nmblookup -A ADRESSE_IP\n"

#: ../ailurus/common/tips.py:145
msgid ""
"Display IP address and MAC\n"
"ifconfig -a\n"
msgstr ""
"Affiche les adresses IP et MAC\n"
"ifconfig -a\n"

#: ../ailurus/common/tips.py:149
msgid ""
"Display route\n"
"netstat -rn\n"
msgstr ""
"Affiche les routes\n"
"netstat -rn\n"

#: ../ailurus/common/tips.py:153
msgid ""
"Set MAC of ethernet interface\n"
"sudo ifconfig eth0 hw ether 00:11:22:33:44:55\n"
msgstr ""
"Régler la MAC de l'interface ethernet\n"
"sudo ifconfig eth0 hw ether 00:11:22:33:44:55\n"

#: ../ailurus/common/tips.py:157
msgid ""
"Display information of a domain name\n"
"whois example.com\n"
msgstr ""
"Affiche les informations d'un nom de domaine\n"
"whois example.com\n"

#: ../ailurus/common/tips.py:161
msgid ""
"Display the network path to a given host\n"
"tracepath example.com\n"
msgstr ""
"Affiche le chemin réseau d'un hôte donné\n"
"tracepath exemple.com\n"

#: ../ailurus/common/tips.py:165
msgid ""
"Request an IP address from DHCP server\n"
"sudo dhclient\n"
msgstr ""
"Demande une adresse IP à un serveur DHCP\n"
"sudo dhclient\n"

#: ../ailurus/common/tips.py:169
msgid ""
"Temporarily restart an init script\n"
"sudo /etc/init.d/SCRIPT_NAME restart\n"
"Temporarily stop an init script\n"
"sudo /etc/init.d/SCRIPT_NAME stop\n"
msgstr ""
"Redémarrer temporairement un script init\n"
"sudo /etc/init.d/SCRIPT_NAME restart\n"
"Stopper temporairement un script init \n"
"sudo /etc/init.d/SCRIPT_NAME stop\n"

#: ../ailurus/common/tips.py:175
msgid ""
"Add a user\n"
"sudo adduser USER_NANE\n"
"Delete a user\n"
"sudo deluser USER_NAME\n"
msgstr ""
"Ajouter un utilisateur\n"
"sudo adduser NOM_UTILISATEUR\n"
"Effacer un utilisateur\n"
"sudo deluser NOM_UTILISATEUR\n"

#: ../ailurus/common/tips.py:181
msgid ""
"Change user password\n"
"sudo passwd USER_NAME\n"
msgstr ""
"Changer le mot de passe d'un utilisateur\n"
"sudo passwd NOM_UTILISATEUR\n"

#: ../ailurus/common/tips.py:185
msgid ""
"Changes user fullname, office number, office extension, and home phone "
"number information.\n"
"sudo chfn USER_NAME\n"
"Display user information\n"
"finger USER_NAME\n"
msgstr ""
"Change les informations utilisateur tels que nom, numéro du bureau, et "
"numéros de maison .\n"
"sudo chfn NOM_UTILISATEUR\n"
"Affiche les informations de l'utilisateur\n"
"finger NOM_UTILISATEUR\n"

#: ../ailurus/common/tips.py:191
msgid ""
"Temporarily prevent a user from logging in\n"
"sudo usermod -L USER_NAME\n"
"Revoke the operation above\n"
"sudo usermod -U USER_NAME\n"
msgstr ""
"Évite temporairement qu'un utilisateur se logue\n"
"sudo usermod -L USER_NAME\n"
"Annule l'opération ci-dessus\n"
"sudo usermod -U USER_NAME\n"

#: ../ailurus/common/tips.py:197
msgid ""
"Add a user to admin group\n"
"sudo usermod -G admin -a USER_NAME\n"
msgstr ""
"Ajouter un utilisateur au groupe administrateur\n"
"sudo usermod -G admin -a NOM_UTILISATEUR\n"

#: ../ailurus/common/tips.py:201
msgid ""
"Set the HTTP proxy\n"
"export http_proxy=http://PROXY.DOMAIN.NAME:PORT\n"
msgstr ""
"Définir le proxy HTTP\n"
"export http_proxy=http://PROXY.DOMAINE.NAME:PORT\n"

#: ../ailurus/common/tips.py:205
msgid ""
"Modify the information displayed after logging in\n"
"sudo vim /etc/motd.tail\n"
msgstr ""
"Modifier les informations affichées après la connexion\n"
"sudo vim /etc/motd.tail\n"

