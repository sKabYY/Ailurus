"údaje.\n"
"cp -a ZDROJOVÁ_SLOŽKA CÍLOVÁ_SLOŽKA\n"

#: ../ailurus/common/tips.py:263
msgid ""
"Determine file type\n"
"file FILE_NAME\n"
msgstr ""
"Určení typu souboru\n"
"file NÁZEV_SOUBORU\n"

#: ../ailurus/common/tips.py:267
msgid ""
"Output the last 6 lines\n"
"tail -n 6 FILE_NAME\n"
msgstr ""
"Výstup posledních 6 řádků\n"
"tail -n 6 NÁZEV_SOUBORU\n"

#: ../ailurus/common/tips.py:271
msgid ""
"Copy files via SSH\n"
"scp -rp FILE_NAME USERNAME@HOST:DEST_PATH\n"
msgstr ""
"Kopírování souborů přes SSH\n"
"scp -rp NÁZEV_SOUBORU UŽIVATELSKÉ_JMÉNO@HOST:CÍLOVÁ_CESTA\n"

#: ../ailurus/common/tips.py:275
msgid ""
"Rename '*.rm' files to '*.rmvb' files\n"
"rename 's/.rm$/.rmvb/' *\n"
msgstr ""
"Přejmenování '*.rm' souborů na '*.rmvb' soubory\n"
"rename 's/.rm$/.rmvb/' *\n"

#: ../ailurus/common/tips.py:279
msgid ""
"Change the file name to lowercase\n"
"rename 'tr/A-Z/a-z/' *\n"
msgstr ""
"Změna názvu souboru na malá písmena\n"
"rename 'tr/A-Z/a-z/' *\n"

#: ../ailurus/common/tips.py:283
msgid ""
"Display subdirectories in current directory\n"
"ls -d */.\n"
msgstr ""
"Zobrazení podsložek v aktuálním složce\n"
"ls -d */.\n"

#: ../ailurus/common/tips.py:287
msgid ""
"Display file number in current directory\n"
"ls . | wc -w\n"
msgstr ""
"Zobrazení čísla souboru v aktuální složce\n"
"ls . | wc -w\n"

#: ../ailurus/common/tips.py:291
msgid ""
"Extract \"*.gz\" file\n"
"gunzip FILE_NAME.gz\n"
"Extract \"*.tar.gz\" file\n"
"tar zxf FILE_NAME.tar.gz\n"
"Extract \"*.tar.bz2\" file\n"
"tar jxf FILE_NAME.tar.bz2\n"
msgstr ""
"Extract \"*.gz\" file\n"
"gunzip FILE_NAME.gz\n"
"Extract \"*.tar.gz\" file\n"
"tar zxf FILE_NAME.tar.gz\n"
"Extract \"*.tar.bz2\" file\n"
"tar jxf FILE_NAME.tar.bz2\n"

#: ../ailurus/common/tips.py:299
msgid ""
"Do compression\n"
"tar czf FILE_NAME.tar.gz FILE1 FILE2 FILE3\n"
"tar cjf FILE_NAME.tar.bz2 FILE1 FILE2 FILE3\n"
msgstr ""
"Vytvoření archívu\n"
"tar czf NÁZEV_SOUBORU.tar.gz SOUBOR1 SOUBOR2 SOUBOR3\n"
"tar cjf NÁZEV_SOUBORU.tar.bz2 SOUBOR1 SOUBOR2 SOUBOR3\n"

#: ../ailurus/common/tips.py:304
msgid ""
"Displays a calendar\n"
"cal\n"
"cal MONTH YEAR\n"
msgstr ""
"Zobrazení kalendáře\n"
"cal\n"
"cal MĚSÍC ROK\n"

#: ../ailurus/common/tips.py:309
msgid ""
"Set the date and time via NTP\n"
"sudo ntpdate ntp.ubuntu.com\n"
msgstr ""
"Nastavení data a času prostřednictvím NTP\n"
"sudo ntpdate ntp.ubuntu.com\n"

#: ../ailurus/common/tips.py:313
msgid ""
"Poweroff your computer\n"
"sudo halt\n"
"sudo shutdown -h now\n"
"Poweroff your computer in 23:00\n"
"sudo shutdown -h 23:00\n"
"Poweroff your computer after 60 minutes\n"
"sudo shutdown -h +60\n"
msgstr ""
"Vypnutí počítače\n"
"sudo halt\n"
"sudo shutdown -h now\n"
"Vypnutí počítače ve 23:00\n"
"sudo shutdown -h 23:00\n"
"Vypnutí počítače po 60 minutách\n"
"sudo shutdown -h +60\n"

#: ../ailurus/common/tips.py:322
msgid ""
"Reboot your computer\n"
"sudo reboot\n"
"sudo shutdown -r now\n"
msgstr ""
"Restartování počítače\n"
"sudo reboot\n"
"sudo shutdown -r now\n"

#: ../ailurus/common/tips.py:327
msgid ""
"If you want some program to start up automatically, please put '.desktop' "
"files into '~/.config/autostart'\n"
msgstr ""
"Pokud chcete nějaký program spouštět automaticky, vložte prosím '.desktop' "
"soubor do '~/.config/autostart'\n"

#: ../ailurus/common/tips.py:330
msgid ""
"You can configure \"preferred applications\" by this file "
"\"~/.local/share/applications/mimeapps.list\"\n"
msgstr ""
"Můžete nastavit \"preferované aplikace\" pomocí tohoto souboru "
"\"~/.local/share/applications/mimeapps.list\"\n"

#: ../ailurus/common/tips.py:333
msgid ""
"Continuously monitor the memory usage\n"
"watch -d free\n"
msgstr ""
"Průběžné sledování využití paměti\n"
"watch -d free\n"

#: ../ailurus/common/tips.py:337
msgid ""
"Display HTTP HEAD response\n"
"w3m -dump_head http://example.com\n"
msgstr ""
"Zobrazení odezvy HTTP HEAD\n"
"w3m -dump_head http://příklad.cz\n"

#: ../ailurus/common/tips.py:341
msgid ""
